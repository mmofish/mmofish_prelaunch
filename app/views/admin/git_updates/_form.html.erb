<%= simple_form_for(@admin_git_update) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :comment %>
    <%= f.input :hashtag %>
    <%= f.input :commit %>
    <%= f.input :commiter %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
