<h1>Listing admin_git_updates</h1>

<table>
  <tr>
    <th>Comment</th>
    <th>Hashtag</th>
    <th>Commit</th>
    <th>Commiter</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_git_updates.each do |admin_git_update| %>
  <tr>
    <td><%= admin_git_update.comment %></td>
    <td><%= admin_git_update.hashtag %></td>
    <td><%= admin_git_update.commit %></td>
    <td><%= admin_git_update.commiter %></td>
    <td><%= link_to 'Show', admin_git_update %></td>
    <td><%= link_to 'Edit', edit_admin_git_update_path(admin_git_update) %></td>
    <td><%= link_to 'Destroy', admin_git_update, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

